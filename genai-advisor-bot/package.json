    "@aws-sdk/client-ssm": "^3.468.0",
    "@aws-sdk/lib-dynamodb": "^3.468.0",
    "aws-amplify": "^6.2.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "node-fetch": "^2.7.0",
    "prompt": "^1.3.0",
    "uuid": "^9.0.1",
    "wickrio_addon": "^6.34.2",
