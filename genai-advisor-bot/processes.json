{
  "apps": [
    {
      "name": "genai-advisor-bot_genai-advisor-bot",
      "args": [],
      "script": "node genai-advisor-bot.js",
      "exec_interpreter": "none",
      "autorestart": true,
      "watch": [
        "package.json"
      ],
      "ignore_watch": [
        ".git"
      ],
      "env": {
        "tokens": {
          "WICKRIO_BOT_NAME": {
            "value": "genai-advisor-bot",
            "encrypted": false
          }
        },
        "log_tokens": {
          "LOG_LEVEL": "debug",
          "LOG_FILE_SIZE": "10m",
          "LOG_MAX_FILES": "5"
        }
      },
      "out_file": "log.output",
      "error_file": "err.output"
    }
  ]
}